{
  "name": "bitrix-http-robot",
  "version": "1.0.0",
  "description": "Custom HTTP Request Robot for Bitrix24",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "install-robot": "node install.js",
    "test": "node --test tests/**/*.test.js",
    "test:services": "node --test tests/services/*.test.js",
    "test:routes": "node --test tests/routes/*.test.js",
    "migrate": "node db/migrate.js",
    "build": "cd admin-ui && npm install && npm run build",
    "oauth": "node oauth-helper.js"
  },
  "keywords": [
    "bitrix24",
    "automation",
    "http",
    "webhook"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.6.0",
    "body-parser": "^1.20.2",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "pg": "^8.18.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  }
}
